<body>
	<!-- 将对应的属性设置到beanwithjsp_02.jsp中的secondUserBean中去 -->
	<form action="/beanwithjsp_02.jsp">
		<!-- 对应User bean中的username属性 -->
		<input name="username" placeholder="username" type="text">

		<!-- 对应User bean中的password属性 -->
		<input name="password" placeholder="password" type="password">
	</form>
</body>
</html>